<view id="root"><view class="header"><block wx:if="{{userInfo.nickName}}"><view class="header-item"><image src="{{userInfo.avatarUrl}}" mode></image><text>{{userInfo.nickName}}</text></view></block><block wx:else><view class="header-item">未登录</view></block><block wx:if="{{userInfo.nickName}}"><view data-event-opts="{{[['tap',[['sigout',['$event']]]]]}}" class="header-item" bindtap="__e">退出登录</view></block><block wx:else><view data-event-opts="{{[['tap',[['longin',['$event']]]]]}}" class="header-item" bindtap="__e">立即登录</view></block></view><view data-event-opts="{{[['tap',[['tosearch',['$event']]]]]}}" class="search" bindtap="__e"><uni-icons vue-id="1da55560-1" type="search" size="25" bind:__l="__l"></uni-icons>搜索歌曲</view><view class="song-list"><block wx:for="{{toplist}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['tosongsheet',['$0'],[[['toplist','id',item.id,'id']]]]]]]}}" class="song-list-item" bindtap="__e"><view class="img"><image src="{{item.coverImgUrl}}" mode="aspectFit"></image><label class="_span">{{item.updateFrequency}}</label></view><view class="song-list-l"><view class="song-list-l-item"><block wx:for="{{item.tracks}}" wx:for-item="son" wx:for-index="index" wx:key="first"><view class="son">{{''+(index+1)+"."+son.first+"-"+son.second+''}}</view></block></view></view></view></block></view></view>